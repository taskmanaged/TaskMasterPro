{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Backend",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/backend/src/index.js",
        "envFile": "${workspaceFolder}/backend/.env",
        "cwd": "${workspaceFolder}/backend"
      },
      {
        "name": "Launch Frontend",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/frontend/src"
      }
    ]
  }
  