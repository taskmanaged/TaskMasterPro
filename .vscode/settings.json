{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
    "prettier.requireConfig": true,
    "files.exclude": {
      "**/__pycache__": true,
      "**/.git": true,
      "**/.svn": true,
      "**/.DS_Store": true
    }
  }
  